{
  "server_address": "0.0.0.0",
  "server_port": "8080",
  "error_template_dir":"config/i18n",
  "error_detail_return": true,
  "log": {
    "level": "[#LOG_LEVEL]",
    "log_dir": "[#WECUBE_GATEWAY_LOG_PATH]",
    "access_log_enable": true,
    "db_log_enable": true,
    "archive_max_size": 64,
    "archive_max_backup": 10,
    "archive_max_day": 15,
    "compress": true
  },
  "remote_service": {
    "route_config_address": "[#GATEWAY_ROUTE_CONFIG_SERVER][#GATEWAY_ROUTE_CONFIG_URI]"
  },
  "redirect_routes": [
    {
      "context": "platform",
      "target_path": "http://[#WECUBE_CORE_HOST]:19100"
    },
    {
      "context": "auth",
      "target_path": "http://[#AUTH_SERVER_HOST]:19120"
    }
  ]
}