{
  "version": "{{version}}",
  "default_language": "en",
  "http_server": {
    "port": "{{http_port}}",
    "cross": false,
    "error_template_dir": "./config/i18n",
    "error_detail_return": true
  },
  "log": {
    "level": "{{log_level}}",
    "log_dir": "logs",
    "access_log_enable": true,
    "db_log_enable": true,
    "metric_log_enable": true,
    "archive_max_size": 200,
    "archive_max_backup": 20,
    "archive_max_day": 30,
    "compress": true,
    "format_json": false
  },
  "database": {
    "server": "{{db_server}}",
    "port": "{{db_port}}",
    "user": "{{db_user}}",
    "password": "{{db_pass}}",
    "database": "{{db_database}}",
    "maxOpen": 200,
    "maxIdle": 50,
    "timeout": 60
  },
  "auth": {
    "enable": true,
    "jwt_signing_key": "",
    "password_seed": "",
    "expire_sec": 600,
    "fresh_token_expire": 3600
  },
  "s3": {
    "server_address": "",
    "access_key": "",
    "secret_key": "",
    "plugin_package_bucket": "wecube-plugin-package-bucket"
  },
  "static_resource": {
    "servers": "",
    "user": "",
    "password": "",
    "port": "",
    "path": ""
  }
}