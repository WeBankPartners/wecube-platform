{
  "server_address": "127.0.0.1",
  "server_port": "[#server_port]",
  "error_template_dir":"config/i18n",
  "error_detail_return": true,
  "log": {
    "level": "[#log_level]",
    "log_dir": "logs",
    "access_log_enable": true,
    "db_log_enable": true,
    "archive_max_size": 64,
    "archive_max_backup": 10,
    "archive_max_day": 15,
    "compress": true
  },
  "remote_service": {
    "route_config_address": "http://[#WECUBE_CORE_HOST]:19100/platform/v1/route-items"
  },
  "redirect_routes": [
    {
      "context": "platform",
      "target_path": "http://[#WECUBE_CORE_HOST]:19100"
    },
    {
      "context": "auth",
      "target_path": "http://[#AUTH_SERVER_HOST]:19120"
    }
  ]
}