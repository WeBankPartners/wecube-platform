{
  "server_address": "127.0.0.1",
  "server_port": "[#server_port]",
  "error_template_dir":"config/i18n",
  "error_detail_return": true,
  "log": {
    "level": "[#log_level]",
    "log_dir": "logs",
    "access_log_enable": true,
    "db_log_enable": true,
    "archive_max_size": 64,
    "archive_max_backup": 10,
    "archive_max_day": 15,
    "compress": true
  },
  "database": {
    "server": "[#db_server]",
    "port": "[#db_port]",
    "user": "[#db_user]",
    "password": "[#db_pass]",
    "database": "[#db_database]",
    "maxOpen": 200,
    "maxIdle": 50,
    "timeout": 60
  },
  "auth":{
    "jwt_private_key_path":"[#admin_jwt_private_key_path]",
    "jwt_public_key_path":"[#admin_jwt_public_key_path]",
    "access_token_mins":[#auth_access_token_mins],
    "refresh_token_hours":[#auth_refresh_token_hours],
    "web_public_key_path":"[#web_public_key_path]",
    "web_private_key_path":"[#web_private_key_path]"
  },
  "umAuth": {
    "address": "[#um_address]",
    "appId": "[#um_app_id]",
    "appToken": "[#um_app_token]"
  },
  "um_permission_upload": {
    "systemId": "[#um_permission_system_id]",
    "address": "[#um_permission_address]",
    "cron_express": "[#um_permission_cron_express]"
  }
}